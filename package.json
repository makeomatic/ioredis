{
  "name": "@makeomatic/ioredis",
  "version": "4.0.0",
  "description": "A robust, performance-focused and full-featured Redis client for Node.js.",
  "main": "built/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "built/"
  ],
  "scripts": {
    "test": "npm run build && cross-env NODE_ENV=test mocha",
    "test:cov": "npm run build && cross-env NODE_ENV=test nyc mocha",
    "build": "rm -rf built && tsc",
    "generate-docs": "jsdoc2md lib/redis.js lib/cluster/index.js lib/commander.js > API.md",
    "prepublishOnly": "npm run build && npm test",
    "bench": "matcha benchmarks/*.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/makeomatic/ioredis.git"
  },
  "keywords": [
    "redis",
    "cluster",
    "sentinel",
    "pipelining"
  ],
  "author": "luin <i@zihua.li> (http://zihua.li)",
  "license": "MIT",
  "dependencies": {
    "cluster-key-slot": "^1.0.12",
    "debug": "^4.0.1",
    "denque": "^1.3.0",
    "event-to-promise": "^0.8.0",
    "flexbuffer": "0.0.6",
    "lodash.bind": "^4.2.1",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.partial": "^4.2.1",
    "lodash.pick": "^4.4.0",
    "lodash.sample": "^4.2.1",
    "lodash.shuffle": "^4.2.0",
    "lodash.values": "^4.3.0",
    "redis-commands": "^1.3.5",
    "redis-errors": "^1.2.0",
    "redis-parser": "^3.0.0",
    "standard-as-callback": "^1.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@types/node": "^10.11.4",
    "bluebird": "^3.5.2",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "semantic-release": "^15.9.17",
    "server-destroy": "^1.0.1",
    "sinon": "^6.3.5",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
